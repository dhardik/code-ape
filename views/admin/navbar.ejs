<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<style>
   * {
      box-sizing: border-box;
   }

   html {
      height: 100%;
      overflow: auto;
   }

   ::-webkit-scrollbar {
      display: none;
   }

   body {
      height: 100%;
      margin: 0;
      padding: 0;
      line-height: 1.42857143;
      background-color: #f1f3f3;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      font-size: 14px;
   }

   .d1 {
      z-index: 10;
      overflow: hidden;
      background-color: #222D3C;
      position: fixed;
      height: 60px;
      z-index: inherit 7;
      width: 100%;
      display: block;
      padding: 0px 15px;
      top: 0;
   }

   #div1-wrapper {
      height: 60px;
   }

   .d1 a {
      float: left;
      color: #f2f2f2;
      text-align: center;
      padding-right: 50px;
      text-decoration: none;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      font-size: 20px;
      font-weight: bold;
   }

   .d1 {
      padding-top: 15px;
      box-sizing: border-box;
   }

   .d1 span {
      color: white;
   }

   .d2 {
      height: 100%;
      width: 50px;
      ;
      position: fixed;
      z-index: 7;
      top: 60px;
      left: 0;
      background-color: #000000;
      overflow: auto;
      transition: 0.5s;
   }

   #div2-wrapper {
      position: absolute;
      display: inline-block;
   }

   .d2 a {
      display: block;
      padding: 11px 4px 11px 15px;
      text-decoration: none;
      color: #999999;
      transition: 0.1s;
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      font-size: 15px;
      white-space: nowrap;
      cursor: pointer;
   }

   .fa {
      font-size: 21px;
      padding-right: 10px;
   }

   .d2 a:hover:not(.active) {
      background-color: #111;
      border-left: 2px solid red;
      color: white;
   }

   .active {
      background-color: #337AB7;
   }

   .active:hover {
      border-left: 2px solid red;
   }

   .d2 img {
      width: 60px;
      height: 60px;
   }

   .act {
      color: white;
   }

   .sadmin {
      float: right;
      padding: .2em .6em .3em;
      font-size: 75%;
      margin-top: 7px;
      font-weight: 700;
      line-height: 1;
      border-radius: 4px;
   }

   .img1 {
      cursor: pointer;
      width: 40px;
      margin-left: 30px;
      margin-right: 10px;
      float: right;
      border-radius: 50%;
   }

   footer {
      position: fixed;
      bottom: 0;
      width: 100%;
      z-index: 7;
   }

   .animdiv {
      position: relative;
      width: 0%;
      height: 0;
      overflow: hidden;
      transition-duration: 0.2s;
      transition-timing-function: cubic-bezier(0.36, 0.55, 0.19, 1);
      transition-property: width, height;
   }

   #div3 {
      background-color: #414F66;
      opacity: 0.95;
   }

   #b1,
   #b2 {
      background-color: #303F53;
      color: white;
      text-shadow: none;
      padding: 10px;
      min-width: 100px;
      font-size: 16px;
      cursor: pointer;
      border: 0;
      border-radius: 2px;
      margin-bottom: 4px;
      margin-left: 2px;
      margin-right: 2px;
   }

   .d4 {
      animation: shake 0.82s infinite;
      transform: translate3d(0, 0, 0);
      backface-visibility: hidden;
      animation-play-state: paused;
   }
</style>
<div class="alert" id="alert_div">
   <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
   <strong>Fill required details !</strong>
</div>
<div id="div1-wrapper">
   <div id="div1" class="d1">
      <a href="javascript:window.location.href=window.location.href">code-ape</a>
      <span style="cursor:pointer;top: 5px;left:30px;position:relative" id="funcnav1" onclick="funcNav()"><i
            class="fa fa-th-large" style="font-size:16px;"></i></span>
      <span class="sadmin" style="background-color: #5cb85c;margin-left:5px"><%= info.role %></span>
      <span class="sadmin" style="background-color: black;"><%= info.name %></span>
   </div>
</div>
<div id="div2-wrapper">
   <div id="div2" class="d2">
      <a id="funcnav2" onclick="funcNav()"><span class="fa"><i class="fa fa-bars"></i></span>cq</a>
      <a class="anc" href="/admin/profile"><span class="fa"><i class="fa fa-home"></i></span>Home</a>
      <a class="anc" href="/admin/adduser"><span class="fa"><i class="fas fa-user-plus"></i></span>Add Students</a>
      <a class="anc" href="/admin/userlist"><span class="fa"><i class="fa fa-list-ul"></i></span>Student List</a>
      <a class="anc" href="/admin/select_topic"><span class="fa"><i class="fa fa-plus-square"></i></span>Add
         Questions</a>
      <a class="anc" href="/practice"><span class="fa"><i class="fas fa-graduation-cap"></i></span>Practice</a>
      <a class="anc" href="/code"><span class="fa"><i class="fas fa-code"></i></span> IDE</a>
      <a class="anc" href="/changepassword"><span class="fa"><i class="fa fa-lock"></i></span>Change Password</a>
      <a onclick="logOut()"><span class="fa"><i class="fa fa-sign-out"></i></span>Logout</a>
   </div>
</div>
<footer>
   <div id="div3" class="animdiv">
      <div id="div4" style="display:table;width:100%;height:100vh;" onclick="ani()" class="d4">
         <div id="div5" style="display:table-cell;text-align:center;margin:auto;vertical-align:middle;">
            <p>
               <font style="font-family:Helvetica;font-size:28px;color:white;">Confirm Logout!</font><br><br>
               <font face="Helvetica" color="white" style="font-size:14px;">Do you really want logout?</font><br><br>
               <button id="b1" onclick="logOut()">Yes</button>
               <button id="b2" onclick="goHome()">No</button>
            </p>
         </div>
      </div>
   </div>
</footer>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="js/vendor/jquery-3.3.1.min.js"><\/script>')</script>
<script type="text/javascript">
   $("#funcnav1").click(function () {
      if ($("#div2").width() == 250) {
         $("#pmain").animate({ left: "50px", }, 400);
      }
      else {
         $("#pmain").animate({ left: "250px", }, 400);
      }
   });
   $("#funcnav2").click(function () {
      if ($("#div2").width() == 250) {
         $("#pmain").animate({ left: "50px", }, 400);
      }
      else {
         $("#pmain").animate({ left: "250px", }, 400);
      }
   });
</script>
<script>
   function funcNav() {
      var div2 = document.getElementById('div2');
      if (div2.style.width == "250px") {
         div2.style.width = "50px";
      }
      else {
         div2.style.width = "250px";
      }
   }
   function goHome() {
      var div3 = document.getElementById('div3');
      div3.style.width = 0;
      div3.style.height = 0;
   }
   function ani() {
      if (event.target == document.getElementById('b2') || event.target == document.getElementById('b1'))
         return;
      document.getElementById("div4").style.animationPlayState = "running";
      setTimeout(function () { document.getElementById("div4").style.animationPlayState = "paused"; }, 820);
   }
   function logOut() {
      if (confirm("Are you sure you want to logout ?")) {
         window.location.href = "/logout";
      }
   }
</script>